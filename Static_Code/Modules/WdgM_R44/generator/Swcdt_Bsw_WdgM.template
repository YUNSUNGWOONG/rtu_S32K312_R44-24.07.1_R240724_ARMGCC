            <DATA-TYPE-MAPPING-SET>
              <SHORT-NAME>WdgM_Mode</SHORT-NAME>
              <MODE-REQUEST-TYPE-MAPS>
                <MODE-REQUEST-TYPE-MAP>
                  <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_DataTypes/ImplementationDataTypes/uint8</IMPLEMENTATION-DATA-TYPE-REF>
                  <MODE-GROUP-REF DEST="MODE-DECLARATION-GROUP">/WdgM/ModeDeclarationGroups/WdgM_Mode</MODE-GROUP-REF>
                </MODE-REQUEST-TYPE-MAP>
              </MODE-REQUEST-TYPE-MAPS>
            </DATA-TYPE-MAPPING-SET>
          </ELEMENTS>
        </AR-PACKAGE>
		<!-- AR PACKAGE ClientServerInterfaces -->
		<AR-PACKAGE>
          <SHORT-NAME>ClientServerInterfaces</SHORT-NAME>
          <ELEMENTS>
            <CLIENT-SERVER-INTERFACE>
               <SHORT-NAME>WdgM_GlobalSupervision</SHORT-NAME>
               <IS-SERVICE>true</IS-SERVICE>
               <OPERATIONS>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>GetFirstExpiredSEID</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Returns SEID that first reached the state WDGM_LOCAL_STATUS_EXPIRED.</L-1>
                        </P>
                     </INTRODUCTION>
                     <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                           <SHORT-NAME>SEID</SHORT-NAME>
                           <INTRODUCTION>
                              <P>
                                 <L-1 L="EN">Identifier of the supervised entity that first reached the state WDGM_LOCAL_STATUS_EXPIRED.</L-1>
                              </P>
                           </INTRODUCTION>
                           <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/WdgM/ImplementationDataTypes/WdgM_SupervisedEntityIdType</TYPE-TREF>
                           <DIRECTION>OUT</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                     </ARGUMENTS>
                     <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_NOT_OK</POSSIBLE-ERROR-REF>
                     </POSSIBLE-ERROR-REFS>
                  </CLIENT-SERVER-OPERATION>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>GetGlobalStatus</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Returns the global supervision status of the Watchdog Manager.</L-1>
                        </P>
                     </INTRODUCTION>
                     <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                           <SHORT-NAME>Status</SHORT-NAME>
                           <INTRODUCTION>
                              <P>
                                 <L-1 L="EN">Global supervision status of the Watchdog Manager.</L-1>
                              </P>
                           </INTRODUCTION>
                           <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/WdgM/ImplementationDataTypes/WdgM_GlobalStatusType</TYPE-TREF>
                           <DIRECTION>OUT</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                     </ARGUMENTS>
                     <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_NOT_OK</POSSIBLE-ERROR-REF>
                     </POSSIBLE-ERROR-REFS>
                  </CLIENT-SERVER-OPERATION>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>GetMode</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Returns the current mode of the Watchdog Manager.</L-1>
                        </P>
                     </INTRODUCTION>
                     <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                           <SHORT-NAME>Mode</SHORT-NAME>
                           <INTRODUCTION>
                              <P>
                                 <L-1 L="EN">Current mode of the Watchdog Manager.</L-1>
                              </P>
                           </INTRODUCTION>
                           <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/WdgM/ImplementationDataTypes/WdgM_ModeType</TYPE-TREF>
                           <DIRECTION>OUT</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                     </ARGUMENTS>
                     <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_NOT_OK</POSSIBLE-ERROR-REF>
                     </POSSIBLE-ERROR-REFS>
                  </CLIENT-SERVER-OPERATION>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>PerformReset</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Instructs the Watchdog Manager to cause a watchdog reset.</L-1>
                        </P>
                     </INTRODUCTION>
                  </CLIENT-SERVER-OPERATION>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>SetMode</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Sets the current mode of Watchdog Manager.</L-1>
                        </P>
                     </INTRODUCTION>
                     <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                           <SHORT-NAME>Mode</SHORT-NAME>
                           <INTRODUCTION>
                              <P>
                                 <L-1 L="EN">One of the configured Watchdog Manager modes.</L-1>
                              </P>
                           </INTRODUCTION>
                           <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/WdgM/ImplementationDataTypes/WdgM_ModeType</TYPE-TREF>
                           <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                     </ARGUMENTS>
                     <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_GlobalSupervision/E_NOT_OK</POSSIBLE-ERROR-REF>
                     </POSSIBLE-ERROR-REFS>
                  </CLIENT-SERVER-OPERATION>
               </OPERATIONS>
               <POSSIBLE-ERRORS>
                  <APPLICATION-ERROR>
                     <SHORT-NAME>E_OK</SHORT-NAME>
                     <DESC>
                        <L-2 L="EN">Operation successful</L-2>
                     </DESC>
                     <ERROR-CODE>0</ERROR-CODE>
                  </APPLICATION-ERROR>
                  <APPLICATION-ERROR>
                     <SHORT-NAME>E_NOT_OK</SHORT-NAME>
                     <DESC>
                        <L-2 L="EN">Operation failed</L-2>
                     </DESC>
                     <ERROR-CODE>1</ERROR-CODE>
                  </APPLICATION-ERROR>
               </POSSIBLE-ERRORS>
            </CLIENT-SERVER-INTERFACE>
            <CLIENT-SERVER-INTERFACE>
               <SHORT-NAME>WdgM_LocalSupervision</SHORT-NAME>
               <IS-SERVICE>true</IS-SERVICE>
               <OPERATIONS>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>CheckpointReached</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Indicates to the Watchdog Manager that a Checkpoint within a Supervised Entity has been reached.</L-1>
                        </P>
                     </INTRODUCTION>
                     <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_LocalSupervision/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_LocalSupervision/E_NOT_OK</POSSIBLE-ERROR-REF>
                     </POSSIBLE-ERROR-REFS>
                  </CLIENT-SERVER-OPERATION>
               </OPERATIONS>
               <POSSIBLE-ERRORS>
                  <APPLICATION-ERROR>
                     <SHORT-NAME>E_OK</SHORT-NAME>
                     <DESC>
                        <L-2 L="EN">Operation successful</L-2>
                     </DESC>
                     <ERROR-CODE>0</ERROR-CODE>
                  </APPLICATION-ERROR>
                  <APPLICATION-ERROR>
                     <SHORT-NAME>E_NOT_OK</SHORT-NAME>
                     <DESC>
                        <L-2 L="EN">Operation failed</L-2>
                     </DESC>
                     <ERROR-CODE>1</ERROR-CODE>
                  </APPLICATION-ERROR>
               </POSSIBLE-ERRORS>
            </CLIENT-SERVER-INTERFACE>
            <CLIENT-SERVER-INTERFACE>
               <SHORT-NAME>WdgM_LocalSupervisionStatus</SHORT-NAME>
               <IS-SERVICE>true</IS-SERVICE>
               <OPERATIONS>
                  <CLIENT-SERVER-OPERATION>
                     <SHORT-NAME>GetLocalStatus</SHORT-NAME>
                     <INTRODUCTION>
                        <P>
                           <L-1 L="EN">Returns the supervision status of an individual Supervised Entity.</L-1>
                        </P>
                     </INTRODUCTION>
                     <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                           <SHORT-NAME>Status</SHORT-NAME>
                           <INTRODUCTION>
                              <P>
                                 <L-1 L="EN">Supervision status of the given supervised entity.</L-1>
                              </P>
                           </INTRODUCTION>
                           <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/WdgM/ImplementationDataTypes/WdgM_LocalStatusType</TYPE-TREF>
                           <DIRECTION>OUT</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                     </ARGUMENTS>
                     <POSSIBLE-ERROR-REFS>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_LocalSupervisionStatus/E_OK</POSSIBLE-ERROR-REF>
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/WdgM/ClientServerInterfaces/WdgM_LocalSupervisionStatus/E_NOT_OK</POSSIBLE-ERROR-REF>
                     </POSSIBLE-ERROR-REFS>
                  </CLIENT-SERVER-OPERATION>
               </OPERATIONS>
               <POSSIBLE-ERRORS>
                  <APPLICATION-ERROR>
                     <SHORT-NAME>E_OK</SHORT-NAME>
                     <DESC>
                        <L-2 L="EN">Operation successful</L-2>
                     </DESC>
                     <ERROR-CODE>0</ERROR-CODE>
                  </APPLICATION-ERROR>
                  <APPLICATION-ERROR>
                     <SHORT-NAME>E_NOT_OK</SHORT-NAME>
                     <DESC>
                        <L-2 L="EN">Operation failed</L-2>
                     </DESC>
                     <ERROR-CODE>1</ERROR-CODE>
                  </APPLICATION-ERROR>
               </POSSIBLE-ERRORS>
            </CLIENT-SERVER-INTERFACE>
          </ELEMENTS>
        </AR-PACKAGE>
        <!-- AR PACKAGE ModeSwitchInterfaces -->
		<AR-PACKAGE>
           <SHORT-NAME>ModeSwitchInterfaces</SHORT-NAME>
           <ELEMENTS>
              <MODE-SWITCH-INTERFACE>
                 <SHORT-NAME>WdgM_GlobalMode</SHORT-NAME>
                 <IS-SERVICE>true</IS-SERVICE>
                 <SERVICE-KIND>WATCH-DOG-MANAGER</SERVICE-KIND>
                 <MODE-GROUP>
                    <SHORT-NAME>currentMode</SHORT-NAME>
                    <TYPE-TREF DEST="MODE-DECLARATION-GROUP">/WdgM/ModeDeclarationGroups/WdgM_Mode</TYPE-TREF>
                 </MODE-GROUP>
              </MODE-SWITCH-INTERFACE>
              <MODE-SWITCH-INTERFACE>
                 <SHORT-NAME>WdgM_LocalMode</SHORT-NAME>
                 <IS-SERVICE>true</IS-SERVICE>
                 <SERVICE-KIND>WATCH-DOG-MANAGER</SERVICE-KIND>
                 <MODE-GROUP>
                    <SHORT-NAME>currentMode</SHORT-NAME>
                    <TYPE-TREF DEST="MODE-DECLARATION-GROUP">/WdgM/ModeDeclarationGroups/WdgM_Mode</TYPE-TREF>
                 </MODE-GROUP>
              </MODE-SWITCH-INTERFACE>
           </ELEMENTS>
        </AR-PACKAGE>
