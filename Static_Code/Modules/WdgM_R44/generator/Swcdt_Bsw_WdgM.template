		<!-- AR PACKAGE ModeDeclarationGroups -->
		<AR-PACKAGE>
           <SHORT-NAME>ModeDeclarationGroups</SHORT-NAME>
           <ELEMENTS>
              <MODE-DECLARATION-GROUP>
                 <SHORT-NAME>WdgM_Mode</SHORT-NAME>
                 <CATEGORY>EXPLICIT_ORDER</CATEGORY>
                 <INITIAL-MODE-REF DEST="MODE-DECLARATION">/WdgM/ModeDeclarationGroups/WdgM_Mode/SUPERVISION_OK</INITIAL-MODE-REF>
                 <MODE-DECLARATIONS>
                    <MODE-DECLARATION>
                       <SHORT-NAME>SUPERVISION_OK</SHORT-NAME>
                       <VALUE>0</VALUE>
                    </MODE-DECLARATION>
                    <MODE-DECLARATION>
                       <SHORT-NAME>SUPERVISION_FAILED</SHORT-NAME>
                       <VALUE>1</VALUE>
                    </MODE-DECLARATION>
                    <MODE-DECLARATION>
                       <SHORT-NAME>SUPERVISION_EXPIRED</SHORT-NAME>
                       <VALUE>2</VALUE>
                    </MODE-DECLARATION>
                    <MODE-DECLARATION>
                       <SHORT-NAME>SUPERVISION_STOPPED</SHORT-NAME>
                       <VALUE>3</VALUE>
                    </MODE-DECLARATION>
                    <MODE-DECLARATION>
                       <SHORT-NAME>SUPERVISION_DEACTIVATED</SHORT-NAME>
                       <VALUE>4</VALUE>
                    </MODE-DECLARATION>
                 </MODE-DECLARATIONS>
                 <ON-TRANSITION-VALUE>255</ON-TRANSITION-VALUE>
              </MODE-DECLARATION-GROUP>
           </ELEMENTS>
        </AR-PACKAGE>
		<!-- AR PACKAGE DataTypeMappingSets -->
		<AR-PACKAGE>
          <SHORT-NAME>DataTypeMappingSets</SHORT-NAME>
